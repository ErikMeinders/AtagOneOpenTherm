{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "/Applications/Arduino.app/Contents/Java/**",
                "/Applications/Arduino.app/Contents/Java/hardware/arduino/**",
                "~/Documents/Arduino/**",
                "~/Documents/Arduino/libraries/**",
                "~/Library/Arduino15/packages/esp8266/tools/**",
                "~/Library/Arduino15/packages/esp8266/hardware/**",
                "~/Library/Arduino15/packages/esp8266/hardware/esp8266/2.6.3/**"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": false,
                "path": [
                    "${workspaceFolder}",
                    "/Applications/Arduino.app/Contents/Java",
                    "/Applications/Arduino.app/Contents/Java/hardware/arduino",
                    "~/Documents/Arduino",
                    "~/Documents/Arduino/libraries",
                    "~/Library/Arduino15/packages/esp8266/tools",
                    "~/Library/Arduino15/packages/esp8266/hardware",
                    "~/Library/Arduino15/packages/esp8266/hardware/esp8266/2.6.3/**"
                ]
            },
            "forcedInclude": [
                "~/Library/Arduino15/packages/esp8266/hardware/esp8266/2.6.3/cores/esp8266/Arduino.h"
            ],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "~/Library/Arduino15/packages/esp8266/tools/xtensa-lx106-elf-gcc/2.5.0-4-b40a506/bin/xtensa-lx106-elf-g++",
            "cStandard": "gnu17",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
